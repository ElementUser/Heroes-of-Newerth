<?xml version="1.0" encoding="UTF-8"?>
<effect name="cyclone">
	<definitions>
		<particlesystem name="system0" space="world" scale="1">
			
			<sound
			    soundloop="true"
			    fadein="200"
			    fadeout="500"
				linearfalloff="true" 
				falloffstart="0"
				minfalloffend="500"
				maxfalloffend="1300"
				volume=".55"
				sample="/heroes/kenisis/ability_02/sounds/tree_float_%.wav"
			/>
			
			<sound
				linearfalloff="true" 
				falloffstart="0"
				minfalloffend="700"
				maxfalloffend="1300"
                minvolume=".7"
				maxvolume="1"
				sample="/heroes/kenisis/ability_02/sounds/throw_%.wav"
			/>
			<model
				position="0 0 50"
				expirelife="200"
				model="tree/model.mdf"
				anim="idle"
				color="1.25 1 .6"
				startscale=".5"
				endscale="0"
				startalpha="1"
				endalpha="0"
				yaw="360"
				yawspeed="180"
				pitchspeed="180"
			>
			</model>
			
			<model
				position="0 0 50"
				expirelife="200"
				model="bark/model.mdf"
				anim="idle"
				color=".8"
				startscale=".65"
				endscale="0"
				startalpha="1"
				endalpha="0"
				yaw="0"
				yawspeed="-180"
				pitchspeed="-180"
			>
			</model>

			<billboard
				expirelife="200"
				position="0 0 50"
				spawnrate="50"
				material="/shared/effects/materials/whitedot.material"
				startsize="150"
				endsize="0"
				lockup="1"
				color="0 .1 .5"
				depthbias="-75"
			/>

			<billboard
				position="0 0 50"
				spawnrate="50"
				life="200"
				material="/shared/effects/materials/flare2.material"
				startheight="0"
				endheight="500"
				width="10"
				roll="90"
				midheightpos=".75"
				lockup="1"
				color="0 .25 .75"
			/>
			
			<model
				position="0 0 50"
				startcolor=".5 1.5 3"
				endcolor="0 0 0"
				model="/heroes/kenisis/ability_04/effects/single_stones/model.mdf"
				material="/shared/effects/materials/shell2.material"
				anim="idle"
				expirelife="200"
				startscale=".32"
				endscale="0"
				yawspeed="180"
				pitchspeed="180"
			>
			</model>

			<model
				position="0 0 50"
				startcolor="0 0 1"
				endcolor="0 0 0"
				model="/heroes/kenisis/ability_04/effects/single_stones/model.mdf"
				material="/shared/effects/materials/shell2.material"
				anim="idle"
				expirelife="200"
				startscale=".2"
				endscale="0"
				yawspeed="-180"
				pitchspeed="-180"
			>
			</model>
					
			<simpleemitter
				position="0 0 50"
				spawnrate="10"
				particlelife="250"
				material="/shared/effects/materials/rad_add.material"
				offsetsphere="5 5 10"
			>
				<particle 
					minangle="0"
					maxangle="360"
					minanglespeed="-25"
					maxanglespeed="25"
					startcolor="0 .2 .6"
					midcolor=".04 .1 .275"
					endcolor="0"
					midcolorpos=".25"
					width="35"
					startsize="100"
					endsize="400"
					lockup="1"
					lockright="1"
					pitch="-90"
					startanchor="1"
					endanchor=".7"
				/>
			</simpleemitter>

		</particlesystem>
	</definitions>
	<thread>
		<spawnparticlesystem instance="instance0" particlesystem="system0" />
		<waitfordeath instance="instance0" />
		<!-- <die /> -->
	</thread>
</effect>

